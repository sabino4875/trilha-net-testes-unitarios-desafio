<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
	<TargetFramework>net8.0</TargetFramework>
	<ImplicitUsings>disable</ImplicitUsings>
	<Nullable>disable</Nullable>
	<PublishRelease>true</PublishRelease>
	<PackageId>TestesUnitarios.Desafio.Console</PackageId>
    <RootNamespace>TestesUnitarios.Desafio.Console</RootNamespace>
    <Version>1.0.1</Version>
	<Authors>Leonardo Buta</Authors>
	<Company>Dio</Company>
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
	<DocumentationFile>documentation/desafio.xml</DocumentationFile>
	<AnalysisMode>All</AnalysisMode>
	<EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>	  
  </PropertyGroup>

</Project>
